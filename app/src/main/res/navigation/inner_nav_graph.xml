<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.chillarcards.privatepractice.ui.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_BookingFragment"
            app:destination="@id/BookingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_StaffBookFragment"
            app:destination="@id/StaffBookFragment" />
        <action
            android:id="@+id/action_home_to_StaffViewBookFragment"
            app:destination="@id/StaffViewBookFragment" />
        <action
            android:id="@+id/action_homeFragment_to_estimateFragment"
            app:destination="@id/estimateFragment" />
        <action
            android:id="@+id/action_homeFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_ReportFragment"
            app:destination="@id/reportFragment" />
        <action
            android:id="@+id/action_estimateFragment_to_SuccessFragment"
            app:destination="@id/successFragment" />
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_homeFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_homeFragment_to_termsAndConditionsFragment"
            app:destination="@id/termsAndConditionsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment" />
        <action
            android:id="@+id/action_homeFragment_to_refundPolicyFragment"
            app:destination="@id/refundPolicyFragment" />
        <action
            android:id="@+id/action_homeFragment_to_contactUsFragment"
            app:destination="@id/contactUsFragment" />
    </fragment>

    <fragment
        android:id="@+id/StaffViewBookFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.BookingViewFragment"
        android:label="StaffAllFragment"
        tools:layout="@layout/fragment_add_staff">
        <action
            android:id="@+id/action_staffFragment_to_BookingFragment"
            app:destination="@id/BookingFragment" />

        <argument
            android:name="staffId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/StaffBookFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.StaffBookFragment"
        android:label="StaffAllFragment"
        tools:layout="@layout/fragment_view_all">
        <action
            android:id="@+id/action_staffFragment_to_BookingFragment"
            app:destination="@id/BookingFragment" />

        <argument
            android:name="staffId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/BookingFragment"
        android:name="com.chillarcards.privatepractice.ui.booking.BookingAllFragment"
        android:label="BookingFragment"
        tools:layout="@layout/fragment_view_all">
        <action
            android:id="@+id/action_OTPFragment_to_RegFragment"
            app:destination="@id/RegisterFragment" />
        <action
            android:id="@+id/action_BookingFragment_to_EstimateFragment"
            app:destination="@id/estimateFragment" />
        <action
            android:id="@+id/action_estimateFragment_to_SuccessFragment"
            app:destination="@id/successFragment" />
        <argument
            android:name="date"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/estimateFragment"
        android:name="com.chillarcards.privatepractice.ui.booking.EstimateFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_estimate">
        <action
            android:id="@+id/action_estimateFragment_to_SuccessFragment"
            app:destination="@id/successFragment" />
        <argument
            android:name="bookId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="docName"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/successFragment"
        android:name="com.chillarcards.privatepractice.ui.booking.SuccessFragment"
        android:label="SuccessFragment"
        tools:layout="@layout/fragment_success">
        <action
            android:id="@+id/action_successFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/walk_book_Fragment"
        android:name="com.chillarcards.privatepractice.ui.appointment.WalkingFragment"
        android:label="addstaffFragment"
        tools:layout="@layout/fragment_walking">
        <action
            android:id="@+id/action_addStaffFragment_to_StaffFragment"
            app:destination="@id/StaffBookFragment" />
        <action
            android:id="@+id/action_walkingFragment_to_EstimateFragment"
            app:destination="@id/estimateFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportFragment"
        android:name="com.chillarcards.privatepractice.ui.report.ReportFragment"
        android:label="addstaffFragment"
        tools:layout="@layout/fragment_report">
    </fragment>
    <fragment
        android:id="@+id/generalFragment"
        android:name="com.chillarcards.privatepractice.ui.general.GeneralFragment"
        android:label="generalFragment"
        tools:layout="@layout/fragment_general">
        <action
            android:id="@+id/action_generalFragment_to_ProfileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_generalFragment_to_bankFragment"
            app:destination="@id/BankFragment" />
        <action
            android:id="@+id/action_generalFragment_to_TimeFragment"
            app:destination="@id/TimeFragment" />
        <action
            android:id="@+id/action_generalFragment_to_RegisterFragment"
            app:destination="@id/RegisterFragment" />

        <action
            android:id="@+id/action_generalFragment_to_ModuleStaffFragment"
            app:destination="@id/StaffModuleFragment" />

        <action
            android:id="@+id/action_generalFragment_to_ModuleServiceFragment"
            app:destination="@id/ServiceModuleFragment" />
        <action
            android:id="@+id/action_generalFragment_to_HomeFragment"
            app:destination="@id/homeFragment" />

    </fragment>
    <fragment
        android:id="@+id/BankFragment"
        android:name="com.chillarcards.privatepractice.ui.register.BankFragment"
        android:label="RegisterFragment"
        tools:layout="@layout/fragment_bank">
        <action
            android:id="@+id/action_BankFragment_to_TimeFragment"
            app:destination="@id/TimeFragment" />
    </fragment>
    <fragment
        android:id="@+id/ServiceModuleFragment"
        android:name="com.chillarcards.privatepractice.ui.service.ServiceFragment"
        android:label="staffFragment"
        tools:layout="@layout/fragment_module_staffs">
        <action
            android:id="@+id/action_StaffFragment_to_AddStaffFragment"
            app:destination="@id/AddStaffFragment" />
    </fragment>
    <fragment
        android:id="@+id/StaffModuleFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.AllStaffFragment"
        android:label="staffFragment"
        tools:layout="@layout/fragment_module_staffs">
        <action
            android:id="@+id/action_StaffFragment_to_AddStaffFragment"
            app:destination="@id/AddStaffFragment" />
    </fragment>
    <fragment
        android:id="@+id/StaffFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.StaffBookFragment"
        android:label="staffFragment"
        tools:layout="@layout/fragment_view_all">
        <action
            android:id="@+id/action_StaffFragment_to_AddStaffFragment"
            app:destination="@id/AddStaffFragment" />
        <action
            android:id="@+id/action_viewStaffFragment_to_StaffFragment"
            app:destination="@id/profileFragment" />

    </fragment>
    <fragment
        android:id="@+id/AddStaffFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.AddStaffFragment"
        android:label="addstaffFragment"
        tools:layout="@layout/fragment_add_staff">
        <action
            android:id="@+id/action_addStaffFragment_to_StaffFragment"
            app:destination="@id/StaffBookFragment" />
    </fragment>
    <fragment
        android:id="@+id/NotificationFragment"
        android:name="com.chillarcards.privatepractice.ui.notification.NotificationFragment"
        android:label="NotificationFragment"
        tools:layout="@layout/fragment_notification">

    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.chillarcards.privatepractice.ui.profile.ProfileFragment"
        android:label="profileFragment"
        tools:layout="@layout/fragment_profile">

        <argument
            android:name="staffId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/TimeFragment"
        android:name="com.chillarcards.privatepractice.ui.register.WorkHoursFragment"
        android:label="WorkHoursFragment"
        tools:layout="@layout/fragment_work_hours">
        <action
            android:id="@+id/action_TimeFragment_to_HomeFragment"
            app:destination="@id/homeBaseFragment" />
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/RegisterFragment"
        android:name="com.chillarcards.privatepractice.ui.register.AccountFragment"
        android:label="RegisterFragment"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/GeneralHomeFragment"
        android:name="com.chillarcards.privatepractice.ui.general.HomeGeneralFragment"
        android:label="HomeGeneralFragment"
        tools:layout="@layout/fragment_general_home" >
        <action
            android:id="@+id/action_generalFragment_to_ProfileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_generalFragment_to_bankFragment"
            app:destination="@id/BankFragment" />
        <action
            android:id="@+id/action_generalFragment_to_TimeFragment"
            app:destination="@id/TimeFragment" />
        <action
            android:id="@+id/action_generalFragment_to_RegisterFragment"
            app:destination="@id/RegisterFragment" />

        <action
            android:id="@+id/action_generalFragment_to_ModuleStaffFragment"
            app:destination="@id/StaffModuleFragment" />

        <action
            android:id="@+id/action_generalFragment_to_ModuleServiceFragment"
            app:destination="@id/ServiceModuleFragment" />
        <action
            android:id="@+id/action_generalFragment_to_HomeFragment"
            app:destination="@id/homeFragment" />

    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.chillarcards.privatepractice.ui.webpage.AboutFragment"
        android:label="AboutFragment" />
    <argument
        android:name="aboutURL"
        app:argType="string" />

    <fragment
        android:id="@+id/termsAndConditionsFragment"
        android:name="com.chillarcards.privatepractice.ui.webpage.TermsAndConditionsFragment"
        android:label="TermsAndConditionsFragment" />
    <argument
        android:name="termsURL"
        app:argType="string" />
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.chillarcards.privatepractice.ui.webpage.PrivacyPolicyFragment"
        android:label="PrivacyPolicyFragment" />
    <argument
        android:name="privacyURL"
        app:argType="string" />
    <fragment
        android:id="@+id/refundPolicyFragment"
        android:name="com.chillarcards.privatepractice.ui.webpage.RefundPolicyFragment"
        android:label="RefundPolicyFragment" />
    <argument
        android:name="refundURL"
        app:argType="string" />
    <fragment
        android:id="@+id/contactUsFragment"
        android:name="com.chillarcards.privatepractice.ui.webpage.ContactUsFragment"
        android:label="ContactUsFragment" />
    <argument
        android:name="contactURL"
        app:argType="string" />

</navigation>