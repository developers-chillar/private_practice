<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mobileFragment">

    <fragment
        android:id="@+id/LoginFragment"
        android:name="com.chillarcards.privatepractice.ui.register.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_signup">
        <action
            android:id="@+id/action_loginFragment_to_mobileFragment"
            app:destination="@id/mobileFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/RegisterLandFragment" />
        <argument
            android:name="isVerifyTrm"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="mobileNo"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_LoginFragment_to_welcomeOnBoardScreenFragment"
            app:destination="@id/welcomeOnBoardScreenFragment" />

    </fragment>

    <fragment
        android:id="@+id/mobileFragment"
        android:name="com.chillarcards.privatepractice.ui.register.MobileFragment"
        android:label="MobileFragment"
        tools:layout="@layout/fragment_mobile">
        <action
            android:id="@+id/action_mobileFragment_to_OTPFragment"
            app:destination="@id/OTPFragment" />
        <argument
            android:name="isVerifyTrm"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="mobileNo"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/OTPFragment"
        android:name="com.chillarcards.privatepractice.ui.register.OTPFragment"
        android:label="OTPFragment"
        tools:layout="@layout/fragment_otp">
        <action
            android:id="@+id/action_OTPFragment_to_HomeFragment"
            app:destination="@id/homeBaseFragment" />
        <action
            android:id="@+id/action_OTPFragment_to_RegFragment"
            app:destination="@id/GeneralHomeFragment" />
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="verificationID"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="resendToken"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/GeneralHomeFragment"
        android:name="com.chillarcards.privatepractice.ui.general.HomeGeneralFragment"
        android:label="HomeGeneralFragment"
        tools:layout="@layout/fragment_general_home" >
        <action
            android:id="@+id/action_generalFragment_to_TimeFragment"
            app:destination="@id/TimeFragment" />
        <action
            android:id="@+id/action_generalFragment_to_RegisterFragment"
            app:destination="@id/RegisterFragment" />
        <action
            android:id="@+id/action_generalFragment_to_HomeFragment"
            app:destination="@id/homeBaseFragment" />

        <action
            android:id="@+id/action_generalFragment_to_bankFragment"
            app:destination="@id/BankFragment" />
    </fragment>

    <fragment
        android:id="@+id/RegisterLandFragment"
        android:name="com.chillarcards.privatepractice.ui.register.GeneralMenuFragment"
        android:label="RegisterLandFragment"
        tools:layout="@layout/fragment_menu" >

    </fragment>

    <fragment
        android:id="@+id/homeBaseFragment"
        android:name="com.chillarcards.privatepractice.ui.home.HomeBaseFragment"
        android:label="HomeBaseFragment"
        tools:layout="@layout/fragment_home_base" >
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_homeBaseFragment_to_mobileFragment"
            app:destination="@id/mobileFragment" />
    </fragment>

    <fragment
        android:id="@+id/RegisterFragment"
        android:name="com.chillarcards.privatepractice.ui.register.AddProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_add_profile" >
        <action
            android:id="@+id/action_RegisterFragment_to_WorkFragment"
            app:destination="@id/TimeFragment" />
        <action
            android:id="@+id/action_RegisterFragment_to_bankFragment"
            app:destination="@id/BankFragment" />
        <action
            android:id="@+id/action_RegisterFragment_to_mapFragment"
            app:destination="@id/MapFragment" />
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/MapFragment"
        android:name="com.chillarcards.privatepractice.ui.register.MapsFragment"
        android:label="MapsFragment"
        tools:layout="@layout/fragment_maps">
        <action
            android:id="@+id/action_MapFragment_to_RegFragment"
            app:destination="@id/RegisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/BankFragment"
        android:name="com.chillarcards.privatepractice.ui.register.BankFragment"
        android:label="RegisterFragment"
        tools:layout="@layout/fragment_bank">
        <action
        android:id="@+id/action_BankFragment_to_TimeFragment"
        app:destination="@id/TimeFragment" />
    </fragment>
    <fragment
        android:id="@+id/TimeFragment"
        android:name="com.chillarcards.privatepractice.ui.register.WorkHoursFragment"
        android:label="WorkHoursFragment"
        tools:layout="@layout/fragment_work_hours">
        <action
            android:id="@+id/action_TimeFragment_to_HomeFragment"
            app:destination="@id/homeBaseFragment" />
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.chillarcards.privatepractice.ui.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_BookingFragment"
            app:destination="@id/BookingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_StaffBookFragment"
            app:destination="@id/StaffBookFragment" />
        <action
            android:id="@+id/action_home_to_StaffViewBookFragment"
            app:destination="@id/StaffViewBookFragment" />
        <action
            android:id="@+id/action_homeFragment_to_estimateFragment"
            app:destination="@id/estimateFragment" />
        <action
            android:id="@+id/action_homeFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_ReportFragment"
            app:destination="@id/reportFragment" />
        <action
            android:id="@+id/action_estimateFragment_to_SuccessFragment"
            app:destination="@id/successFragment" />
        <argument
            android:name="mobileNo"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/StaffViewBookFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.BookingViewFragment"
        android:label="StaffAllFragment"
        tools:layout="@layout/fragment_add_staff">
        <action
            android:id="@+id/action_staffFragment_to_BookingFragment"
            app:destination="@id/BookingFragment" />

        <argument
            android:name="staffId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/StaffBookFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.StaffBookFragment"
        android:label="StaffAllFragment"
        tools:layout="@layout/fragment_view_all">
        <action
            android:id="@+id/action_staffFragment_to_BookingFragment"
            app:destination="@id/BookingFragment" />

        <argument
            android:name="staffId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/BookingFragment"
        android:name="com.chillarcards.privatepractice.ui.booking.BookingAllFragment"
        android:label="BookingFragment"
        tools:layout="@layout/fragment_view_all">
        <action
            android:id="@+id/action_OTPFragment_to_RegFragment"
            app:destination="@id/RegisterFragment" />
        <action
            android:id="@+id/action_BookingFragment_to_EstimateFragment"
            app:destination="@id/estimateFragment" />
        <action
            android:id="@+id/action_estimateFragment_to_SuccessFragment"
            app:destination="@id/successFragment" />
        <argument
            android:name="date"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/estimateFragment"
        android:name="com.chillarcards.privatepractice.ui.booking.EstimateFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_estimate">
        <action
            android:id="@+id/action_estimateFragment_to_SuccessFragment"
            app:destination="@id/successFragment" />
        <argument
            android:name="bookId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="docName"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/successFragment"
        android:name="com.chillarcards.privatepractice.ui.booking.SuccessFragment"
        android:label="SuccessFragment"
        tools:layout="@layout/fragment_success">
        <action
            android:id="@+id/action_successFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/walk_book_Fragment"
        android:name="com.chillarcards.privatepractice.ui.appointment.WalkingFragment"
        android:label="addstaffFragment"
        tools:layout="@layout/fragment_walking">
        <action
            android:id="@+id/action_addStaffFragment_to_StaffFragment"
            app:destination="@id/StaffBookFragment" />
        <action
            android:id="@+id/action_walkingFragment_to_EstimateFragment"
            app:destination="@id/estimateFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportFragment"
        android:name="com.chillarcards.privatepractice.ui.report.ReportFragment"
        android:label="addstaffFragment"
        tools:layout="@layout/fragment_report">
    </fragment>
    <fragment
        android:id="@+id/generalFragment"
        android:name="com.chillarcards.privatepractice.ui.general.GeneralFragment"
        android:label="generalFragment"
        tools:layout="@layout/fragment_general">
        <action
            android:id="@+id/action_generalFragment_to_ProfileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_generalFragment_to_bankFragment"
            app:destination="@id/BankFragment" />
        <action
            android:id="@+id/action_generalFragment_to_TimeFragment"
            app:destination="@id/TimeFragment" />
        <action
            android:id="@+id/action_generalFragment_to_RegisterFragment"
            app:destination="@id/RegisterFragment" />

        <action
            android:id="@+id/action_generalFragment_to_ModuleStaffFragment"
            app:destination="@id/StaffModuleFragment" />

        <action
            android:id="@+id/action_generalFragment_to_ModuleServiceFragment"
            app:destination="@id/ServiceModuleFragment" />
        <action
            android:id="@+id/action_generalFragment_to_HomeFragment"
            app:destination="@id/homeFragment" />

    </fragment>

    <fragment
        android:id="@+id/ServiceModuleFragment"
        android:name="com.chillarcards.privatepractice.ui.service.ServiceFragment"
        android:label="staffFragment"
        tools:layout="@layout/fragment_module_staffs">
        <action
            android:id="@+id/action_StaffFragment_to_AddStaffFragment"
            app:destination="@id/AddStaffFragment" />
    </fragment>
    <fragment
        android:id="@+id/StaffModuleFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.AllStaffFragment"
        android:label="staffFragment"
        tools:layout="@layout/fragment_module_staffs">
        <action
            android:id="@+id/action_StaffFragment_to_AddStaffFragment"
            app:destination="@id/AddStaffFragment" />
    </fragment>
    <fragment
        android:id="@+id/StaffFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.StaffBookFragment"
        android:label="staffFragment"
        tools:layout="@layout/fragment_view_all">
        <action
            android:id="@+id/action_StaffFragment_to_AddStaffFragment"
            app:destination="@id/AddStaffFragment" />
        <action
            android:id="@+id/action_viewStaffFragment_to_StaffFragment"
            app:destination="@id/profileFragment" />

    </fragment>
    <fragment
        android:id="@+id/AddStaffFragment"
        android:name="com.chillarcards.privatepractice.ui.staffs.AddStaffFragment"
        android:label="addstaffFragment"
        tools:layout="@layout/fragment_add_staff">
        <action
            android:id="@+id/action_addStaffFragment_to_StaffFragment"
            app:destination="@id/StaffBookFragment" />
    </fragment>
    <fragment
        android:id="@+id/NotificationFragment"
        android:name="com.chillarcards.privatepractice.ui.notification.NotificationFragment"
        android:label="NotificationFragment"
        tools:layout="@layout/fragment_notification">

    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.chillarcards.privatepractice.ui.profile.ProfileFragment"
        android:label="profileFragment"
        tools:layout="@layout/fragment_profile">

        <argument
            android:name="staffId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/welcomeOnBoardScreenFragment"
        android:name="com.chillarcards.privatepractice.ui.selfonboard.WelcomeOnBoardScreenFragment"
        android:label="WelcomeOnBoardScreenFragment" >
        <action
            android:id="@+id/action_welcomeOnBoardScreenFragment_to_mobileFragment"
            app:destination="@id/mobileFragment" />
    </fragment>

</navigation>